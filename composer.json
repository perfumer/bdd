{
  "name": "perfumer/contracts",
  "type": "library",
  "description": "Code generating tool",
  "keywords": ["generator"],
  "homepage": "https://github.com/perfumer/contracts",
  "license": "MIT",
  "authors": [
    {
      "name": "Ilyas Makashev",
      "email": "mehmatovec@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "phpunit/phpunit": "~5 || ~6",
    "doctrine/annotations": "~1",
    "friendsofphp/php-cs-fixer": "~2",
    "zendframework/zend-code": "^3.1"
  },
  "autoload-dev": {
    "psr-4": {
      "Perfumer\\Contracts\\Example\\": "example/src/",
      "Tests\\Perfumer\\Contracts\\Example\\": "example/tests/",
      "Generated\\Perfumer\\Contracts\\Example\\": "example/generated/src/",
      "Generated\\Tests\\Perfumer\\Contracts\\Example\\": "example/generated/tests/"
    }
  },
  "autoload": {
    "psr-4": {
      "Perfumer\\Contracts\\": "src/"
    }
  },
  "config": {
    "preferred-install": "dist"
  }
}
